        <config name="MemoryScene">
            <item scene="idle">
                <action>reclaim</action>
            </item>
            <item scene="default">
                <action>kill</action>
            </item>
            <item scene="BigMem">
                <action>quickkill</action>
            </item>
            <item scene="Emergency">
                <action>reclaimPersist</action>
            </item>
        </config>
    </policy>
